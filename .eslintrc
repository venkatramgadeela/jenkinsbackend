{
  "extends": [
    "airbnb",
    "plugin:prettier/recommended",
    "prettier/react",
    "react-app"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": ["Link"],
        "specialLink": ["to"]
      }
    ]
  }
}
